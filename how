[36m@@ -0,0 +1,82 @@[m
[32m+[m[32mclass PinsController < ApplicationController[m
[32m+[m[32m  before_action :set_pin, only: [:show, :edit, :update, :destroy][m
[32m+[m[32m  before_action :authenticate_user!, except: [:index, :show][m[41m [m
[32m+[m[32m  before_action :correct_user, only: [:edit, :update, :destroy][m
[32m+[m
[32m+[m[32m  # GET /pins[m
[32m+[m[32m  # GET /pins.json[m
[32m+[m[32m  def index[m
[32m+[m[32m    @pins = Pin.all[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # GET /pins/1[m
[32m+[m[32m  # GET /pins/1.json[m
[32m+[m[32m  def show[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # GET /pins/new[m
[32m+[m[32m  def new[m
[32m+[m[32m    @pin = current_user.pins.build[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # GET /pins/1/edit[m
[32m+[m[32m  def edit[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # POST /pins[m
[32m+[m[32m  # POST /pins.json[m
[32m+[m[32m  def create[m
[32m+[m[32m    @pin = current_user.pins.build(pin_params)[m
[32m+[m
[32m+[m[32m    respond_to do |format|[m
[32m+[m[32m      if @pin.save[m
[32m+[m[32m        format.html { redirect_to @pin, notice: 'Pin was successfully created.' }[m
[32m+[m[32m        format.json { render :show, status: :created, location: @pin }[m
[32m+[m[32m      else[m
[32m+[m[32m        format.html { render :new }[m
[32m+[m[32m        format.json { render json: @pin.errors, status: :unprocessable_entity }[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # PATCH/PUT /pins/1[m
[32m+[m[32m  # PATCH/PUT /pins/1.json[m
[32m+[m[32m  def update[m
[32m+[m[32m    respond_to do |format|[m
[32m+[m[32m      if @pin.update(pin_params)[m
[32m+[m[32m        format.html { redirect_to @pin, notice: 'Pin was successfully updated.' }[m
[32m+[m[32m        format.json { render :show, status: :ok, location: @pin }[m
[32m+[m[32m      else[m
[32m+[m[32m        format.html { render :edit }[m
[32m+[m[32m        format.json { render json: @pin.errors, status: :unprocessable_entity }[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # DELETE /pins/1[m
[32m+[m[32m  # DELETE /pins/1.json[m
[32m+[m[32m  def destroy[m
[32m+[m[32m    @pin.destroy[m
[32m+[m[32m    respond_to do |format|[m
[32m+[m[32m      format.html { redirect_to pins_url, notice: 'Pin was successfully destroyed.' }[m
[32m+[m[32m      format.json { head :no_content }[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m[41m  [m
[32m+[m[32m  def correct_user[m
[32m+[m[32m    @pin =current_user.pins.find_by(id:params[:id])[m
[32m+[m[32m        redirect_to pins_path, notice: "Not authorized to edit this pin" if @pin.nil?[m
[32m+[m[32m  end[m[41m [m
[32m+[m[41m  [m
[32m+[m[41m [m
[32m+[m[32m  private[m
[32m+[m[32m    # Use callbacks to share common setup or constraints between actions.[m
[32m+[m[32m    def set_pin[m
[32m+[m[32m      @pin = Pin.find(params[:id])[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Never trust parameters from the scary internet, only allow the white list through.[m
[32m+[m[32m    def pin_params[m
[32m+[m[32m      params.require(:pin).permit(:description)[m
[32m+[m[32m    end[m
[32m+[m[32mend[m
