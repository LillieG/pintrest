[36m@@ -0,0 +1,58 @@[m
[32m+[m[32mclass PinsController < ApplicationController[m
[32m+[m	[32mbefore_action :find_pin, only: [:show, :edit, :update, :destroy, :upvote, :downvote][m
[32m+[m	[32mbefore_action :authenticate_user!, except: [:index, :show][m
[32m+[m
[32m+[m	[32mdef index[m
[32m+[m		[32m@pins = Pin.all.order("created_at DESC")[m
[32m+[m	[32mend[m
[32m+[m
[32m+[m	[32mdef show[m
[32m+[m	[32mend[m
[32m+[m
[32m+[m	[32mdef new[m
[32m+[m		[32m@pin = current_user.pins.build[m
[32m+[m	[32mend[m
[32m+[m
[32m+[m	[32mdef create[m
[32m+[m		[32m@pin = current_user.pins.build(pin_params)[m
[32m+[m
[32m+[m		[32mif @pin.save[m
[32m+[m			[32mredirect_to @pin, notice: "Pin was successfully created"[m
[32m+[m		[32melse[m
[32m+[m			[32mrender 'new'[m
[32m+[m		[32mend[m
[32m+[m	[32mend[m
[32m+[m
[32m+[m	[32mdef edit[m
[32m+[m	[32mend[m
[32m+[m
[32m+[m	[32mdef update[m
[32m+[m		[32mif @pin.update(pin_params)[m
[32m+[m			[32mredirect_to @pin, notice: "Pin was successfully updated"[m
[32m+[m		[32melse[m
[32m+[m			[32mrender 'edit'[m
[32m+[m		[32mend[m
[32m+[m	[32mend[m
[32m+[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m[32mdef update[m
[32m+[m		[32mif @pin.destroy(pin_params)[m
[32m+[m			[32mredirect_to @pin, notice: "Pin was successfully deleted"[m
[32m+[m		[32melse[m
[32m+[m			[32mrender 'edit'[m
[32m+[m		[32mend[m
[32m+[m	[32mdef upvote[m
[32m+[m		[32m@pin.upvote_by current_user[m
[32m+[m		[32mredirect_to :back[m
[32m+[m	[32mend[m
[32m+[m
[32m+[m	[32mprivate[m
[32m+[m
[32m+[m	[32mdef pin_params[m
[32m+[m		[32mparams.require(:pin).permit(:title, :description, :image)[m
[32m+[m	[32mend[m
[32m+[m
[32m+[m	[32mdef find_pin[m
[32m+[m		[32m@pin = Pin.find(params[:id])[m
[32m+[m	[32mend[m
