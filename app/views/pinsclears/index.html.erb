<p id="notice"><%= notice %></p>

<h1>Listing Pinsclears</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pinsclears.each do |pinsclear| %>
      <tr>
        <td><%= link_to 'Show', pinsclear %></td>
        <td><%= link_to 'Edit', edit_pinsclear_path(pinsclear) %></td>
        <td><%= link_to 'Destroy', pinsclear, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pinsclear', new_pinsclear_path %>
